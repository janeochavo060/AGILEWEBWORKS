    "predev": "node lib/preBuildScripts/preBuildDev.js",
    "prebuild": "node lib/preBuildScripts/preBuildDev.js",
    "postbuild": "next-sitemap",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "release-it",
    "prepare": "husky",
    "test": ""
  },
  "dependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@fancyapps/ui": "^5.0.36",
    "@marsidev/react-turnstile": "^0.1.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "fs": "^0.0.1-security",
    "husky": "^9.0.11",
    "jsona": "^1.11.0",
    "moment-timezone": "^0.5.41",
    "next": "^13.5.8",
    "next-sitemap": "^3.1.45",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-select": "^5.7.0",
    "release": "^6.3.1",
    "sharp": "^0.31.3",
    "swiper": "^11.1.11",
    "swr": "^2.0.0",
    "util": "^0.12.5",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": "conventionalcommits",
        "types": [
          {
            "type": "feat",
            "section": "Features"
          },
          {
            "type": "fix",
            "section": "Bug Fixes"
          }
        ]
      }
    }
  }
}
